#tui.uedog.com 绑定子目录tui
RewriteEngine On
RewriteCond %{HTTP_HOST} ^tui\.uedog\.com$
RewriteCond %{REQUEST_URI} !^/tui/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /tui/$1
RewriteCond %{HTTP_HOST} ^tui\.uedog\.com$
RewriteRule ^(/)?$ tui/index.php [L]

#pixwind.com 绑定子目录pix
RewriteEngine On
RewriteCond %{HTTP_HOST} ^((www|)\.)?pixwind\.com$
RewriteCond %{REQUEST_URI} !^/pix/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /pix/$1
RewriteCond %{HTTP_HOST} ^((www|)\.)?pixwind\.com$
RewriteRule ^(/)?$ pix/index.php [L]
